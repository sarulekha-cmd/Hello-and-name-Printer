import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      Scanner a=new Scanner(System.in);
        int num1=a.nextInt();
        double num2=a.nextDouble();
        System.out.println(num1);
        System.out.printf("%.2f%n",num2);
        a.close();
    }
}
